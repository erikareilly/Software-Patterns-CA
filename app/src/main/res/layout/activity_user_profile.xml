<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserProfile">

    <LinearLayout
        android:id="@+id/linear"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:orientation="horizontal">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="202dp"
        android:layout_height="180dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="104dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.118"
        app:srcCompat="@drawable/icon" />
    </LinearLayout>



    <TextView
        android:id="@+id/nameDisplay"
        android:layout_width="125dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@id/linear"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="266dp"
        android:layout_marginBottom="414dp"
        android:text="Name: "
        android:textColor=" #000080"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/emailDisplay"
        android:layout_width="112dp"
        android:layout_height="57dp"
        android:layout_below="@id/numberDisplay"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="279dp"
        android:layout_marginBottom="264dp"
        android:text="Email: "
        android:textColor=" #000080"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/numberDisplay"
        android:layout_width="159dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_below="@id/nameDisplay"
        android:layout_marginTop="334dp"
        android:layout_marginEnd="232dp"
        android:layout_marginBottom="353dp"
        android:text="PhoneNumber: "
        android:textColor=" #000080"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.633" />

    <TextView
        android:id="@+id/name"
        android:layout_width="141dp"
        android:layout_height="60dp"
        android:layout_below="@id/linear"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="189dp"
        android:layout_marginTop="251dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="419dp"
        android:text=""
        android:textColor="#000080"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.803"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505" />

    <TextView
        android:id="@+id/number"
        android:layout_width="124dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="324dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="337dp"
        android:layout_below="@id/name"
        android:text=""
        android:textColor="#000080"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.815"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.638" />

    <TextView
        android:id="@+id/emailText"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/number"
        android:layout_marginStart="153dp"
        android:layout_marginTop="411dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="250dp"
        android:text=""
        android:textColor="#000080"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/admin"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="510dp"
        android:layout_below="@id/emailDisplay"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="173dp"
        android:text="User Type:"
        android:textSize="20dp"
        android:textColor="#000080"/>

    <TextView
        android:id="@+id/userDef"
        android:layout_width="203dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="172dp"
        android:layout_below="@id/emailText"
        android:layout_marginTop="505dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="162dp"
        android:text=""
        android:textColor="#000080"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="127dp"
        android:layout_below="@id/emailText"
        android:layout_marginTop="618dp"
        android:layout_marginEnd="128dp"
        android:backgroundTint="#000080"
        android:layout_marginBottom="54dp"
        android:text="Verify" />


</RelativeLayout>